{
    "name": "berarma/cakephp-redsys",
    "description": "Redsys plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "GPL-2.0",
    "keywords": ["cakephp", "redsys", "tpv", "servired", "payments", "card"],
    "homepage": "https://github.com/berarma/cakephp-redsys",
    "authors": [
        {
            "name": "Bernat Arlandis",
            "email": "berarma@hotmail.com"
        }
    ],
    "require": {
        "cakephp/cakephp": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Berarma\\Redsys\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Berarma\\Redsys\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
